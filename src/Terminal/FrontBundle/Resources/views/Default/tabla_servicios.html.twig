{% for entity in servicios %}
            <tr>
                <td><strong>{{ entity.empresa | raw}}</strong></td>
                <td><strong>{{ entity.nombre | raw}}</strong></td>                            
                <td><strong>{% if entity.hora %}{{ entity.hora|date('H:i') }}{% endif %}</strong></td>                            
                <td><strong>{{ entity.plataforma }}</strong></td>                            
                <td><span class="label {{ entity.estadoColor ? 'label-' ~ entity.estadoColor : '' }}">{{ entity.estadoTexto }}</span></td>                            
            </tr>
{% endfor %}
